				float crossYDotWorldDerivX34_g1 = abs( dotResult6_g1 );
				float2 temp_cast_1 = (_RefractionScale).xx;
				float mulTime39 = _TimeParameters.x * _RefractionSpeed;
				float2 temp_cast_2 = (mulTime39).xx;
				float2 texCoord33 = packedInput.ase_texcoord7.xy * temp_cast_1 + temp_cast_2;
